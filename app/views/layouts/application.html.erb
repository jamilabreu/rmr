<html>
<head>
	<script type="text/javascript" src="http://use.typekit.com/dkg3ynx.js"></script>
	<script type="text/javascript">try{Typekit.load();}catch(e){}</script>
 	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<title>Jamilabreu</title>
	<meta name="description" content="">
	<meta name="author" content="Jamil Abreu">
	<meta name="viewport" content="width=device-width">
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render "layouts/header" %>
<div class="container">
	<%= content_tag :p, notice, :class => 'alert alert-block' if notice.present? %> 
	<%= content_tag :p, alert, :class => 'alert alert-error' if alert.present? %>
	<%= yield %>
</div>
<%= render "layouts/analytics" if Rails.env.production? %>
</body>
</html>
